{
  "name": "mymdb",
  "version": "1.0.0",
  "description": "full stack capstone",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client/mymdb",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client/mymdb/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "Bryan Johnson",
  "license": "ISC",
  "cacheDirectories": [
    "node_modules",
    "client/mymdb/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ]
}
